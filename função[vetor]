#include<stdio.h>
void vetor(int tamanho){
  int maior=0, vet[tamanho];
  for(int i=0;i<tamanho;i++){
    printf("Digite um numero: \n");
    scanf("%d", &vet[i]);
  } for(int i=0;i<tamanho;i++){
    if(vet[i]>maior){
      maior=vet[i];
    }
  }printf("O maior numero Ã© o: %d", maior);
}
int main(){
 int t;
  printf("Digite o tamanho do vetor: ");
  scanf("%d", &t);
  vetor(t);
}
